{
  "extends": [
    "@tsconfig/strictest"
  ],
  "include": [
    "../src/**/*"
  ],
  "compilerOptions": {
    "lib": [
      "DOM",
      "ESNext"
    ],
    "jsx": "preserve",
    "noEmit": true,
    "target": "es2020",
    "module": "esnext",
    "moduleResolution": "bundler",
    "erasableSyntaxOnly": true
  }
}
